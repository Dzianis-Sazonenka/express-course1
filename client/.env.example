# ====================================
# NASA Missions App - Client Environment Variables
# ====================================
# Copy this file to .env and update the values

# API Base URL (point to your backend server)
REACT_APP_API_URL=http://localhost:8000

# Google OAuth Client ID (for client-side OAuth if needed)
REACT_APP_GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com

# Google Analytics Tracking ID (optional)
# REACT_APP_GA_TRACKING_ID=UA-XXXXXXXX-X

# Sentry DSN (for error tracking, optional)
# REACT_APP_SENTRY_DSN=https://examplePublicKey@o0.ingest.sentry.io/0

# Environment (development, test, production)
NODE_ENV=development

# HTTPS (set to true in production)
HTTPS=false

# Enable/disable debug logging
REACT_APP_DEBUG=true

# Feature Flags (optional)
# REACT_APP_FEATURE_NEW_DASHBOARD=false
# REACT_APP_FEATURE_EXPERIMENTAL=false

# API Timeout (in milliseconds)
REACT_APP_API_TIMEOUT=10000

# WebSocket URL (if using WebSockets)
# REACT_APP_WS_URL=ws://localhost:8000/ws

# Mapbox Access Token (if using maps)
# REACT_APP_MAPBOX_TOKEN=your-mapbox-token

# Version (auto-populated by build process)
# REACT_APP_VERSION=$npm_package_version
